#include<cstdio>
#include<cmath>
#include<cstring>
#include<algorithm>
#include<string>
#include<iostream>
#include<cstdlib>
#include<vector>
#include<stack>
using namespace std;

int main() {
	int c;
	scanf("%d\n", &c);
	long long ans1 = -2^32+ c;
	long long ans2 = 2^32 - 1 - c;
	printf("%lld\n", ans2 - ans1);
	return 0;
}
